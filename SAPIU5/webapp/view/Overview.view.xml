<mvc:View
    controllerName="logaligroup.SAPUI5.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true">
    <Page id="_IDGenPage1" title="{i18n>appTitle}">
        <landmarkInfo>
            <PageAccessibleLandmarkInfo id="_IDGenPageAccessibleLandmarkInfo" 
                rootRole="Region"
                rootLabel="{i18n>overviewRootLabel}"
                contentRole="Main"
                contentLabel="{i18n>overviewContentLabel}"
                headerRole="Banner"
                headerLabel="{i18n>iverviewHeaderLabel}"/>
        </landmarkInfo>
        <headerContent>
            <Button
                id="helloDialogButtonHeader"
                icon="sap-icon://action"
                press=".onOpenDialogHeader"
                class="sapUiSmallMarginEnd"/>
        </headerContent>
        <content>
            <mvc:XMLView id="_IDGenXMLView2" viewName="logaligroup.SAPUI5.view.HelloPanel"/>
            <mvc:XMLView id="_IDGenXMLView3" viewName="logaligroup.SAPUI5.view.InvoicesList"/>
        </content>
    </Page>
</mvc:View>
